                </dict>
                <dict>
                        <key>Description</key>
                        <string>Method</string>
                        <key>Identifier</key>
                        <string>SharedInstanceName</string>
                        <key>Default</key>
                        <string>sharedInstance</string>
                        <key>Name</key>
                        <string>Shared Instance Name:</string>
                        <key>Required</key>
                        <string>YES</string>
                        <key>Type</key>
                        <string>text</string>
                        <key>NotPersisted</key>
                        <true/>
                </dict>
        </array>
</dict>
